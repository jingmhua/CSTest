@model IEnumerable<WebApplication_Null.Models.Student>
<!DOCTYPE html>
@{ Layout = "~/Views/Shared/_Layout.cshtml"; ViewBag.Title = "学生列表页";}

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>名字</th>
            <th>班级</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>
                    @student.id
                </td>
                <td>
                    @student.Name
                </td>
                <td>
                    @student.ClassName
                </td>
            </tr>
        }
    </tbody>
</table>

@section Scripts{ 
    <script src="~/js/CustomScript.js"></script>
}
